<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.Student" table="student" schema="onelecture">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="studentName">
            <column name="student_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="studentNickname">
            <column name="student_nickname" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="studentPassword">
            <column name="student_password" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="studentMajor">
            <column name="student_major" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="studentEmail">
            <column name="student_email" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>